{
  "name": "renum",
  "version": "1.0.0",
  "description": "Simple enum using frozen object.",
  "main": "lib/index.js",
  "jsnext:main": "es/index.js",
  "files": [
    "es",
    "lib",
    "src"
  ],
  "config": {
    "reporter": "spec",
    "source_files": "package.json src/index.js src/*.js",
    "test_files": "src/__test__/*-test.js",
    "bench_files": "src/__bench__/*-bench.js"
  },
  "scripts": {
    "env": "env",
    "mocha": "mocha",
    "matcha": "matcha",
    "jsdoc2md": "jsdoc2md",
    "lint": "eslint src/*.js",
    "bench": "matcha $npm_package_config_bench_files",
    "test": "cross-env BABEL_ENV=commonjs mocha --compilers js:babel-register $npm_package_config_test_files --reporter $npm_package_config_reporter",
    "docs": "jsdoc2md --verbose --conf docs/jsdoc.json -t docs/README.hbs $npm_package_config_source_files > README.md; echo",
    "coverage": "istanbul cover _mocha -- $npm_package_config_test_files --reporter $npm_package_config_reporter",
    "build": "npm run build:es && npm run build:commonjs",
    "build:es": "cross-env BABEL_ENV=es babel src -d es",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src -d lib",
    "prepublish": "npm run test && npm run build",
    "watch:docs": "nodemon $npm_package_config_source_files docs/README.hbs --delay 250ms -x 'npm run docs'",
    "watch:test": "npm run test -- -w",
    "watch:coverage": "nodemon $npm_package_config_test_files $npm_package_config_source_files --delay 2500ms -x 'npm run coverage'",
    "watch:lint": "nodemon src/*.js -x 'eslint'"
  },
  "private": true,
  "keywords": [
    "enum",
    "constants",
    "freeze",
    "immutable",
    "ramda",
    "redux"
  ],
  "author": "bydooweedoo <bydooweedoo@gmail.com>",
  "license": "PRIVATE",
  "dependencies": {
    "ramda": "^0.19.1"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.7.0",
    "babel-preset-es2015": "^6.6.0",
    "cross-env": "^1.0.7",
    "eslint": "^2.3.0",
    "eslint-config-airbnb": "^6.1.0",
    "expect": "1.14.x",
    "expect-enum": "file:../expect-enum",
    "immutable": "^3.7.6",
    "istanbul": "^0.4.2",
    "jsdoc-to-markdown": "^1.3.3",
    "matcha": "^0.6.1",
    "mocha": "^2.4.5",
    "nodemon": "^1.8.1",
    "ramda-debug": "^0.1.3"
  }
}
